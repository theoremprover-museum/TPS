@Marker(Library,BtpsDocuments,Dover)
@Style(Date "1952 March 8", Font TimesRoman10,
	Doublesided On,
	LeftMargin 1.25inches, RightMargin 1.25inches,
	BottomMargin 1.0inches, FileDate "March 8, 1952")
@Define(T, FaceCode F)
@Define(CB, Font SmallBodyFont, FaceCode B)
@Define(TPSExample, Use Verbatim, Font SmallBodyFont, FaceCode F,
	LeftMargin +0.5inches)

@CommandString(LISP="@b[L]@cb[ISP]", MACLISP="@b[M]@cb[AC]@LISP")
@CommandString(TPS="@b[T]@cb[PS]", ETPS="@b[E]@cb[TPS]", BTPS="@b[B]@cb[TPS]")
@CommandString(GRADER="@b[G]@cb[RADER]", REPORT="@b[R]@cb[EPORT]")
@CommandString(hGRADER="G@c[RADER]")
@CommandString(REVIEW="@b[R]@cb[EVIEW]")

@string(copyright-and-Research-Credit=
"@copyrightnotice(Carnegie Mellon University.  All rights reserved.)
@begin(researchcredit)
This manual is based upon work supported by
NSF grants MCS81-02870, DCR-8402532, CCR-8702699, and 
CCR-9002546,
and a grant from Center for Design of Educational Computing,
Carnegie Mellon University. Any opinions, findings, and conclusions or
recommendations are those of the author(s) and do not necessarily reflect
the views of the National Science Foundation.
@end(researchcredit)")
@String(NSF="NSF grants MCS81-02870, DCR-8402532, and CCR-8702699")

@TextForm{
  AppBTPS="@PageHeading(Right <@B[Appendix @Ref{Appendix}: @parm{text}]>,
                        Left <@b(@Value(DocName))>, Even)
    @Appendix(@parm{text})
    @PageHeading(Right < >, immediate)
    @PageHeading(Right <@b(Appendix @Ref(Appendix): @parm{text})>, 
                 Left <@b(@Value(DocName))>, Odd)
    @Case<DoubleSided,
       No [@PageHeading(Right <@b(Appendix @Ref{Appendix}: @parm{text})>,
                        Left <@b(@Value(DocName))>, Even)],
       Else [@PageHeading(Left <@b(Appendix @Ref{Appendix}: @parm{text})>, 
  	                  Right <@b(@Value(DocName))>, Even)]>"}
@CommandString{StopHeaders="@PageHeading(Right < >)"}
@TextForm{
  ChapBTPS="@PageHeading(Right <@B[Chapter @Ref{Chapter}: @parm{text}]>,
                        Left <@b(@Value(DocName))>, Even)
    @Chapter(@parm{text})
    @PageHeading(Right < >, immediate)
    @PageHeading(Right <@b(Chapter @Ref(Chapter): @parm{text})>, 
                 Left <@b(@Value(DocName))>, Odd)
    @Case<DoubleSided,
       No [@PageHeading(Right <@b(Chapter @Ref{Chapter}: @parm{text})>,
                        Left <@b(@Value(DocName))>, Even)],
       Else [@PageHeading(Left <@b(Chapter @Ref{Chapter}: @parm{text})>, 
  	                  Right <@b(@Value(DocName))>, Even)]>",
  NotAvailable="@b([N/A])@index(Not Available)"}
@PageFooting(Center="@Value(Page)")
@CommandString(it = "@ref(TPSLineCounter)")
@Counter(TpsLineCounter, Numbered [<@1>  ], Referenced [<@1>  ], IncrementedBy
	 Reference)
@Define(Tasks, Use Description, Counter TpsLineCounter, Spread 0.3)
@TextForm(uxt = [@t{@ux@parmquote{text}}])
@TextForm(wt = "@w{@t@ParmQuote[Text]}")
@TextForm(IndText="@parm(text)@index@parmquote(text)")
@Define(LispCode, Use verbatim, font bodyfont, facecode f,
	LeftMargin +0.5inches)
@CommandString( 0inf="@!@+{n}@/@-{0} ",
		1inf="@!@+{n}@/@-{1} ")

@LibraryFile(KSets)

@LibraryFile(index)

@Specialfont(f9="SailA10",f1="TimesRoman10I",f2="ARK12S10")
@Define(TB, FaceCode T)
@Define(Hand, use text, indent -14 points, afterentry="
@f2[R]")
@Define(Exempli, use text, indent 0 points, afterentry="

@tabclear
@b[EXAMPLE]@ @ @ @ ")

@CommandString(Gratia="@\ ")

@TextForm(GrVar="@w{@p(@parm[text])}")
@TextForm(GrCom="@w{@cb(@parm[text])}")
@TextForm(BTPSCom="@GrCom(@parm[text])")
@TextForm(GrVarI="@GrVar[@parm(text)]@IndexEntry{Key=@parm(text), 
	Entry=@GrVar[@parm(text)], Number}")
@TextForm(GrComI="@GrCom[@parm(text)]@IndexEntry{Key=@parm(text), 
	Entry=@GrCom[@parm(text)], Number}")
@TextForm(BTPSComI="@BTPSCom[@parm(text)]@IndexEntry{Key=@parm(text), 
	Entry=@BTPSCom[@parm(text)], Number}")
@TextForm(RevCom="@BTPSCom[@parm(text)]")
@Textform(RevComI="@BTPSComI[@parm(text)]")
@TextForm(TConText="@parm[text]@Send(Contents @parmquote[text])")
@TextForm(Warning="@f2(D) @parm[text]")



